                    </button>
              
                <div class="collapse navbar-collapse d-lg-flex justify-content-md-end" id="navbarTogglerDemo02">
                  <ul class="justify-content-end navbar-nav">
                   
                       <li class="nav-item d-flex align-items-center">
                        <p class="mb-0 mr-3">Already have an account?</p>
                        <a class="btn btn-primary" href="login-new.html">Sign In</a> 
                      </li>
                     
                  </ul>
                </div>
               
                
              </nav>
              

              <div class="login-wrapper">


                <div class="w-100">
                    <div class="row justify-content-center">
                        <div class="col-12 col-md-11 col-lg-9 col-xl-6">
                            <div class="box-shadow-1">

                                <a class="brand" href="index.html">
                                    <img src="{{ baseUrl }}/app/dist/images/logo2.png" class="rounded" width="70" alt="...">
                                </a>

                                <p class="mt-4">
                                    <span style="max-width: 400px" class="d-block m-auto" > Enter your e-mail and password below to log in to your account. </span>
                                   
                                </p>

                                <div class="box-shadow-1-main register mt-4">
                                    <form action="">
                                      <div class="row">
                                        <div class="col-12 col-md-6">
                                              <div class="form-wrap has-error mb-3">
                                                <input class="form-input" id="register-sponsor" placeholder="Sponsor ID" type="text" name="sponsor">
                                                <span class="form-validation">The text field is required.</span>
                                            </div>

                                            <div class="form-wrap has-error mb-3">
                                                <input class="form-input" id="register-firstname" placeholder="First Name" type="text" name="firstname">
                                                <span class="form-validation">The text field is required.</span>
                                            </div>

                                            <div class="form-wrap has-error mb-3">
                                              <input class="form-input" id="register-lastname" placeholder="Last Name" type="email" name="lastname">
                                              <span class="form-validation">The text field is required.</span>
                                            </div>
                                            <div class="form-wrap has-error mb-3">
                                              <input class="form-input" id="register-email" placeholder="Email" type="email" name="email">
                                              <span class="form-validation">The text field is required.</span>
                                            </div>

                                        </div>

                                        <div class="col-12 col-md-6">

                                          <div class="form-wrap has-error mb-3">
                                            <input class="form-input" id="register-phone" placeholder="Phone Number" type="text" name="number">
                                            <span class="form-validation">The text field is required.</span>
                                        </div>
    
                                        <div class="form-wrap has-error mb-3">
                                          <input class="form-input" id="register-country" placeholder="Country" type="text" name="country">
                                          <span class="form-validation">The text field is required.</span>
                                      </div>
    
                                          <div class="form-wrap has-error mb-3">
                                              <input class="form-input" id="register-password" placeholder="Password" type="password" name="password">
                                              <span class="form-validation">The text field is required.</span>
                                          </div>
    
                                          <div class="form-wrap has-error mb-3">
                                            <select class="">
                                              <option>Bussiness Partner</option>
                                              <option>Customer</option>
                                          </select>
                                            <span class="form-validation">The text field is required.</span>
                                        </div>

                                        </div>

                                      </div>
                                      

                                  

                                      

                                        <div class="form-wrap box-shadow-1-main mt-3">
                                            <a href="dashboard/members.html" class="btn w-100 btn-primary button-shadow">
                                              Register
                                            </a>
                                          </div>


                                    </form>
                                </div>



                            </div>

                        </div>

                    </div>
                </div>
               
              </div>

            
          </div>
        </div>


    

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
