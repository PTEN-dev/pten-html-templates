    {% block stylesheet %}{% endblock %}


    <title>Register | PTEN</title>
  </head>
  <body id="login">
        <div class="wrapper">
          <div class="content">
            <nav class="navbar px-2 position-relative px-lg-5 navbar-expand-lg bg-white main">
              
                  <a class="d-flex" href="{{ baseUrl }}/html/index.html">
                     <span class="mr-1"><i class="fa fa-arrow-left"></i></span> Back 
                  </a>
                 
                   
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
                      <span class="navbar-toggler-icon">
                        <svg xmlns="http://www.w3.org/2000/svg" width="29.649" height="16.273" viewBox="0 0 29.649 16.273">
